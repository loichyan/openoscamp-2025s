{
  "version": "0.2.0",
  "configurations": [
    {
      "name": "(GDB) Kernel",
      "type": "cppdbg",
      "request": "launch",
      "cwd": "${workspaceFolder}",
      "MIMode": "gdb",
      "miDebuggerPath": "riscv-none-elf-gdb",
      "miDebuggerServerAddress": "localhost:1234",
      "program": "${workspaceFolder}/target/riscv64gc-unknown-none-elf/debug/kernel",
      "setupCommands": [
        {
          "description": "Enable pretty-printing for gdb",
          "text": "-enable-pretty-printing",
          "ignoreFailures": true
        }
      ],
      "preLaunchTask": "make:run-qemu"
    }
  ]
}
